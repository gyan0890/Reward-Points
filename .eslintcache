                active: tabs === 2,\n                      })}\n                      onClick={(e) => {\n                        e.preventDefault();\n                        setTabs(2);\n                      }}\n                      href=\"#pablo\"\n                    >\n                      Exchange\n                    </NavLink>\n                  </NavItem>\n                </Nav>\n                <TabContent\n                  className=\"tab-subcategories\"\n                  activeTab={\"tab\" + tabs}\n                >\n                  <TabPane tabId=\"tab1\">\n                    <Table className=\"tablesorter\" responsive>\n                      <tbody>\n                        <tr>\n                          <td>OWNER</td>\n                          <td>{nft.token_id}</td>\n                        </tr>\n                        <tr>\n                          <td>TOKEN URL</td>\n                          <td>{nft.token_url}</td>\n                        </tr>\n                      </tbody>\n                    </Table>\n                  </TabPane>\n                  <TabPane tabId=\"tab2\">\n                    <Row>\n                      <Label sm=\"3\">Auction Period</Label>\n                      <Col sm=\"9\">\n                        <FormGroup>\n                          <Input\n                            placeholder=\"e.g. e7364bn\"\n                            type=\"text\"\n                            onChange={(event) =>\n                              setAuctionPeriod(event.target.value)\n                            }\n                          />\n                          <FormText color=\"default\" tag=\"span\">\n                            Please enter Auction Period in Seconds.\n                          </FormText>\n                        </FormGroup>\n                      </Col>\n                    </Row>\n                    <Row>\n                      <Label sm=\"3\">Send to</Label>\n                      <Col sm=\"9\">\n                        <FormGroup>\n                          <FormText tag=\"span\">NFT Address: {}</FormText>\n                        </FormGroup>\n                      </Col>\n                    </Row>\n                    <Row>\n                      <Label sm=\"3\">Send to</Label>\n                      <Col sm=\"9\">\n                        <FormGroup>\n                          <FormText tag=\"span\">\n                            Token ID: {nft.token_id}\n                          </FormText>\n                        </FormGroup>\n                      </Col>\n                    </Row>\n                    <Button\n                      className=\"btn-simple btn-icon btn-round float-right\"\n                      color=\"primary\"\n                      type=\"submit\"\n                      onClick={() => onExchangeRequest(item.contract_address,nft, auctionPeriod)}\n                    >\n                      <i className=\"tim-icons icon-send\" />\n                    </Button>\n                  </TabPane>\n                </TabContent>\n              </CardBody>\n            </Card>\n          </Col>\n        );\n        //return <li> {nft.owner} as the {nft.token_url} </li>\n      });\n    }\n  });\n  if (allNFTdata.length > 0) {\n    const rows = Math.ceil(allNFTdata.length / 3);\n    let counter = 0;\n    for (let index = 0; index < rows; index++) {\n      const tempArray = [];\n      for (let j = 0; j < 3; j++) {\n        tempArray.push(allNFTdata[counter++]);\n      }\n      rowColumnData.push(tempArray);\n    }\n    displayGrid = rowColumnData.map((d) => {\n        return (\n          <Row>\n            {d.map((nft) => {\n              if (nft){\n              return (\n                <Col className=\"ml-auto mr-auto\" lg=\"4\" md=\"6\">\n                  <Card className=\"card-coin card-plain\">\n                    <CardHeader>\n                      <img\n                        alt=\"...\"\n                        className=\"img-center